      <media:thumbnail url="https://us.hola.com/images/026a-1298e3e20840-bec8e5e454ea-1000/horizontal-1200/on-the-set-of-quot-indiana-jones-and-the-temple-of-doom-quot-.jpg" type="image/jpeg"></media:thumbnail>
      <media:content url="https://us.hola.com/images/026a-1298e3e20840-bec8e5e454ea-1000/horizontal-1200/on-the-set-of-quot-indiana-jones-and-the-temple-of-doom-quot-.jpg" type="image/jpeg" width="1200" height="720" medium="image">
